@model Rossell_HO.ViewModels.TeaReceiptView

    <select name="docno" id="docno" class="form-control">
        <option value="0">-- Select --</option>
        @foreach (var item in Model.DOCNO_TEA)
        {
            if (TempData["rec_tea_doccode"] != null)
            {
                if (TempData["rec_tea_doccode"].ToString() == item.DOCNO)
                {
                <option value="@item.DOCNO" selected="selected">@item.DOCNO.ToString()</option>
                }
                else
                {
                    <option value="@item.DOCNO">@item.DOCNO.ToString()</option>
                }
            }
            else
            {
                <option value="@item.DOCNO">@item.DOCNO.ToString()</option>
            }
        }
    </select>
